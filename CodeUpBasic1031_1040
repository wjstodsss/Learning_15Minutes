//CodeUp1031
//10진수 정수하나를 입력받아 8진수로 출력하기
import java.util.Scanner;

public class Java1031 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int decimal = scanner.nextInt();
		System.out.printf("%o", decimal);
		scanner.close();
	}
}
//CodeUp1032
//10진수 정수하나를 입력받아 16진수로 출력하기
import java.util.Scanner;

public class Java1032 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int decimal = scanner.nextInt();
		System.out.printf("%x", decimal);
		scanner.close();
	}
}
//CodeUp1033
//10진수 정수하나를 입력받아 16진수로 대문자 출력하기
import java.util.Scanner;

public class Java1033 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int decimal = scanner.nextInt();
		System.out.printf("%X", decimal);
		scanner.close();
	}
}
//CodeUp1034
//8진수 정수하나를 입력받아 10진수로 출력하기
import java.util.Scanner;

public class Java1034 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String octalNumber = scanner.next();
		System.out.print(Integer.valueOf(octalNumber,8));
		scanner.close();
	}
}
//CodeUp1035
//16진수 정수하나를 입력받아 8진수로 출력하기
import java.util.Scanner;

public class Java1035 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String octalNumber = scanner.next();
		System.out.printf(Integer.toOctalString(Integer.valueOf(octalNumber,16)));
		scanner.close();
	}
}
//CodeUp1036
//영문자 1개 입력받아 10진수로 출력하기
import java.util.Scanner;
public class Java1036 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		char a = scanner.nextLine().charAt(0);
		System.out.print((int)a);
		scanner.close();
	}

}
//CodeUp1037
//정수1개 입력받아 아스키 문자로 출력하기
import java.util.Scanner;
public class Java1037 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		System.out.print((char)a);
		scanner.close();
	}

}
//CodeUp1038
//정수 2개 입력받아 합 출력하기
import java.util.Scanner;
public class Java1038 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		System.out.println((long)a+b);
		scanner.close();
		
	}
}
//CodeUp1039
//정수 2개 입력받아 합 출력하기
import java.util.Scanner;
public class Java1039 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		long a = scanner.nextLong();
		long b = scanner.nextLong();
		System.out.println((long)(a+b));
		scanner.close();
		
	}
}
//CodeUp1040
//정수 1개 입력받아 부호 바궈 출력하기
import java.util.Scanner;
public class Java1040 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		System.out.println(-1*a);
		scanner.close();
	}

}

