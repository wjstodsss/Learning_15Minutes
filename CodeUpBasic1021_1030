//단어를 입력받아 그대로 출력하기
import java.util.Scanner;

public class java1021 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String Word = "";
		Word = scanner.nextLine();
		System.out.printf("%s", Word);
		scanner.close();
		
	}
}
